{
	"name": "progopener",
	"author": "sugjo",
	"version": "0.0.1",
	"main": "src/electron/electron.cjs",
	"scripts": {
		"dev": "vite dev",
		"preview": "vite preview",
		"electron": "set NODE_ENV=development&& concurrently --kill-others \"vite dev --port 3000\" \"electron src/electron/electron.cjs\"",
		"build": "vite build",
		"dev:package": "npm run build && npm run clear-icons && electron-builder --config electron-builder.config.json --dir",
		"package": "npm run build && electron-builder --config electron-builder.config.json",
		"lint": "prettier --check --plugin-search-dir=. .",
		"format": "prettier --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@sveltejs/vite-plugin-svelte": "^1.0.1",
		"concurrently": "^7.2.2",
		"electron": "^19.0.8",
		"electron-builder": "^23.1.0",
		"electron-reloader": "^1.2.3",
		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.7.0",
		"svelte": "^3.44.0",
		"svelte-check": "^2.7.1",
		"vite": "^3.0.0"
	},
	"type": "module",
	"dependencies": {
		"electron-json-storage": "^4.5.0",
		"electron-serve": "^1.1.0",
		"electron-window-manager": "^1.0.6",
		"electron-window-state": "^5.0.3",
		"emulate-key-in-browser": "^1.0.0-rc.1",
		"extract-file-icon": "^0.3.2",
		"i18next": "^21.8.16",
		"i18next-browser-languagedetector": "^6.1.4",
		"i18next-http-backend": "^1.4.1",
		"mz": "^2.7.0",
		"ospath": "^1.2.2",
		"svelte-i18next": "^1.2.2"
	}
}
